{
  "version": "0.2",
  "language": "en",
  "enableFiletypes": [
    "markdown",
    "mdx",
    "typescript",
    "typescriptreact",
    "javascript",
    "javascriptreact"
  ],
  "ignorePaths": [
    ".cspell.json",
    ".github/workflows/**",
    ".vscode/*.json",
    "**/dist/**",
    "**/node_modules/**",
    "pnpm-lock.yaml"
  ],
  "dictionaries": [
    "typescript",
    "softwareTerms",
    "node",
    "en_US",
    "npm",
    "misc",
    "filetypes"
  ],
  "ignoreRegExpList": [
    "/```[\\w\\W]*?```/",
    "/~~~[\\w\\W]*?~~~/",
    "/``[\\w\\W]*?``/",
    "/`[^`]*`/",
    "/\\.\\/docs\\/rules\\/[^.]*.md/",
    "/@typescript-eslint\\/[a-z-]+/",
    "/\\.all-contributorsrc/",
    "/TS[^\\s]+/",
    "\\(#.+?\\)"
  ],
  "words": ["tseslint"],
  "overrides": [
    {
      "filename": "**/*.{ts,mts,cts,js,cjs,mjs,tsx,jsx}",
      "ignoreRegExpList": ["/@[a-z]+/", "/#(end)?region/"],
      "includeRegExpList": [
        "/\\/\\*[\\s\\S]*?\\*\\/|([^\\\\:]|^)\\/\\/.*$/",
        "/(\\/\\/[^\\n\\r]*[\\n\\r]+)/"
      ]
    }
  ]
}
